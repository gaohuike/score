#create the standard answer to list S
f = open("StandardAnswer.txt")
lines = f.readlines()
S = []
for line in lines:
    S.append(line)
f.close()

#create someone's answer to list A
T = open("SomeoneAnswer.txt")
answers = T.readlines()
A = []
for answer in answers:
    A.append(answer)
T.close()

#compare the answers and calculate the scores
L=len(S)
score=0
for i in range(L):
	if S[i]==A[i]:
		score=score+3
	else:
		score=score-2
print score
